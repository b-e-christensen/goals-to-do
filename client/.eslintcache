te the `authLink` middleware prior to making the request to our GraphQL API\n  link: authLink.concat(httpLink),\n  cache: new InMemoryCache(),\n});\n\nconst AuthContext = React.createContext(null);\n\nfunction App() {\n\n  return (\n    <ApolloProvider client={client}>\n      <Router>\n        <div className=\"flex-column justify-flex-start min-100-vh\">\n          <Header />\n          <div className=\"container\">\n            <Routes>\n              <Route \n                path=\"/\" \n                element={<Start />}\n              />\n              <Route \n                path=\"/login\" \n                element={<Login />}\n              />\n              <Route \n                path=\"/signup\" \n                element={<Signup />}\n              />\n              <Route \n                path=\"/dashboard\" \n                element={<Dashboard />}\n              />\n            </Routes>\n          </div>\n          <Footer />\n        </div>\n      </Router>\n    </ApolloProvider>\n  );\n}\n\nexport default App;\n","/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/components/Header.js",[],"/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/components/Footer.js",[],"/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/pages/Login.js",[],"/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/utils/mutations.js",[],"/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/utils/auth.js",[],"/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/components/Start.js",[],"/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/pages/Signup.js",[],"/Users/malaphar/Documents/bootcamp/DU-DEN-VIRT-FSF-PT-12-2021-U-C/Projects/goals-to-do/client/src/pages